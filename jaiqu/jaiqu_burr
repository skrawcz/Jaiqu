digraph {
	graph [compound=false concentrate=false rankdir=TB ranksep=0.4]
	validate_input_schema [label=validate_input_schema shape=box style=rounded]
	error_state [label=error_state shape=box style=rounded]
	create_jq_filter_query [label=create_jq_filter_query shape=box style=rounded]
	compile_and_test_jq_filter [label=compile_and_test_jq_filter shape=box style=rounded]
	retry_create_jq_filter [label=retry_create_jq_filter shape=box style=rounded]
	validate_json [label=validate_json shape=box style=rounded]
	retry_validate_json [label=retry_validate_json shape=box style=rounded]
	final_jq_query_string [label=final_jq_query_string shape=box style=rounded]
	validate_input_schema -> error_state [label="valid_input_schema=False" style=dashed]
	validate_input_schema -> create_jq_filter_query [label="valid_input_schema=True" style=dashed]
	create_jq_filter_query -> compile_and_test_jq_filter [style=solid]
	compile_and_test_jq_filter -> retry_create_jq_filter [label="status=Fail" style=dashed]
	retry_create_jq_filter -> compile_and_test_jq_filter [label="status=Success" style=dashed]
	retry_create_jq_filter -> error_state [label="status=Failed n times" style=dashed]
	compile_and_test_jq_filter -> validate_json [label="status=Success" style=dashed]
	validate_json -> final_jq_query_string [label="status=Success" style=dashed]
	validate_json -> retry_validate_json [label="status=Fail" style=dashed]
	retry_validate_json -> final_jq_query_string [label="status=Success" style=dashed]
	retry_validate_json -> error_state [label="status=Failed n times" style=dashed]
}
